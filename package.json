{
  "name": "json-node-editor",
  "version": "0.1.0",
  "private": true,
  "keywords": [
    "node-editor",
    "node",
    "editor",
    "json",
    "schema",
    "retejs"
  ],
  "description": "Visual node based editor for JSON data",
  "homepage": "https://github.com/joeledwardson/JSON-node-editor",
  "types": "./types/app.d.ts",
  "main": "./dist/app.js",
  "dependencies": {
    "ajv": "^8.10.0",
    "xlsx-column": "^2.2.1"
  },
  "scripts": {
    "webpack-serve": "webpack serve",
    "build": "npm run build:css && npm run build:src && npm run build:types",
    "build:css": "node-sass src/styles/ -o dist/",
    "build:src": "tsc -p tsconfig.json",
    "build:types": "tsc -p configs/tsconfig.types.json",
    "dev": "watch 'npm run build' src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "rete": "^1.4.5",
    "rete-react-render-plugin": "github:joeledwardson/react-render-plugin#build"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "bootstrap": "^5.1.3",
    "jest": "^27.5.1",
    "node-sass": "^7.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-fontawesome": "^1.7.1",
    "react-select": "^5.2.2",
    "react-textarea-autosize": "^8.3.3",
    "rete": "^1.4.5",
    "rete-react-render-plugin": "github:joeledwardson/react-render-plugin#build",
    "typescript": "^4.4.4",
    "watch": "^1.0.2"
  }
}
